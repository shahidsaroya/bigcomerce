{"components/cart/item-options":" <form method=\"post\" action=\"{{urls.cart}}\" enctype=\"multipart/form-data\">\n    <input type=\"hidden\" name=\"item_id\" value=\"{{item.id}}\">\n    <p> Options\n        {{#each product.options}}\n            <div>{{{dynamicComponent 'components/products/options'}}}</div>\n        {{/each}}\n    </p>\n    <input type=\"submit\" class=\"btn btn-success\" value=\"Save\">\n</form>\n","components/products/options/date":"<div class=\"form-field\" data-product-attribute=\"date\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" {{#if style}}style=\"{{style}}\" {{/if}}>\n        {{this.display_name}}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][month]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.month'}}</option>\n        {{#for 1 12}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.month '==' $index}}selected=\"selected\"{{/if}}>\n                {{lang (concat 'common.short_months.' $index)}}\n            </option>\n        {{/for}}\n    </select>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][day]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.day'}}</option>\n        {{#for 1 31}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.day '==' $index}}selected=\"selected\"{{/if}}>\n                {{$index}}\n            </option>\n        {{/for}}\n    </select>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][year]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.year'}}</option>\n        {{#for earliest_year latest_year}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.year '==' $index}}selected=\"selected\"{{/if}}>\n                {{$index}}\n            </option>\n        {{/for}}\n        {{#if earliest_year '==' latest_year}}\n        <option value=\"{{earliest_year}}\" selected=\"selected\">{{earliest_year}}</option>\n        {{/if}}\n    </select>\n</div>\n","components/products/options/input-checkbox":"<div class=\"form-field\" data-product-attribute=\"input-checkbox\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{display_name}}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n    <input type=\"hidden\" name=\"attribute[{{id}}]\" value=\"{{noValue}}\" />\n    <input\n        class=\"form-checkbox\"\n        type=\"checkbox\"\n        name=\"attribute[{{id}}]\"\n        id=\"attribute-check-{{id}}\"\n        value=\"{{value}}\"\n        {{#if checked}}\n            checked\n            data-default\n        {{/if}}\n        {{#if required}}required{{/if}}>\n\n    <label class=\"form-label {{class}}\" for=\"attribute-check-{{id}}\">{{label}}</label>\n</div>\n","components/products/options/input-file":"<div class=\"form-field\" data-product-attribute=\"input-file\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_file_{{id}}\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <input class=\"form-file\" type=\"file\" id=\"attribute_file_{{id}}\" name=\"attribute[{{this.id}}]\" {{#if required}}required{{/if}}>\n    {{#if original_name}}\n        {{{lang 'products.file_option_set' url=file_url name=original_name}}}\n        <label>{{lang 'cart.remove_file'}}<input type=\"checkbox\" name=\"{{file_remove}}\"></label>\n    {{/if}}\n    <p class=\"form-fileDescription\">{{#if file_size}}{{lang 'products.max_filesize'}} <strong>{{file_size}}</strong>, {{/if}}{{#if file_types}}{{lang 'products.filetypes'}} <strong>{{file_types}}</strong>{{/if}}</p>\n\n</div>\n","components/products/options/input-numbers":"<div class=\"form-field\" data-product-attribute=\"input-number\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_number_{{id}}\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <input class=\"form-input\" type=\"number\" id=\"attribute_number_{{id}}\" {{#unless this.integer_only}}step=\"any\"{{/unless}} name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\n</div>\n","components/products/options/input-text":"<div class=\"form-field\" data-product-attribute=\"input-text\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_text_{{id}}\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <input class=\"form-input form-input--small\" type=\"text\" id=\"attribute_text_{{id}}\" name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}} {{#if max_length}}maxlength=\"{{max_length}}\"{{/if}}>\n</div>\n","components/products/options/product-list":"<div class=\"form-field\" data-product-attribute=\"product-list\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{display_name}}:\r\n        <span data-option-value></span>\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    {{#if values.0.image}}\r\n        <ul class=\"productOptions-list\">\r\n            {{#unless required}}\r\n                <li class=\"productOptions-list-item\">\r\n                    <input class=\"form-radio\"\r\n                           type=\"radio\"\r\n                           name=\"attribute[{{id}}]\"\r\n                           value=\"0\"\r\n                           id=\"attribute_productlist_0_{{id}}\"\r\n                           checked=\"{{#if defaultValue '==' 0}}checked{{/if}}\" required>\r\n                    <label class=\"form-label\" for=\"attribute_productlist_0_{{id}}\">{{lang 'products.none'}}</label>\r\n                </li>\r\n            {{/unless}}\r\n            {{#each values}}\r\n                <li class=\"productOptions-list-item\" data-product-attribute-value=\"{{id}}\">\r\n                    {{#if image}}\r\n                        <figure class=\"productOptions-list-item-figure\">\r\n                            <img class=\"productOptions-list-item-image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage image 'productthumb_size'}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\r\n                        </figure>\r\n                    {{/if}}\r\n                    <div class=\"productOptions-list-item-content\">\r\n                        <input\r\n                            class=\"form-radio\"\r\n                            type=\"radio\"\r\n                            name=\"attribute[{{../id}}]\"\r\n                            value=\"{{id}}\"\r\n                            id=\"attribute_productlist_{{../id}}_{{id}}\"\r\n                            {{#if selected}}\r\n                                checked\r\n                                data-default\r\n                            {{/if}}\r\n                            {{#if ../required}}required{{/if}}>\r\n                        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_productlist_{{../id}}_{{id}}\">{{label}}</label>\r\n                    </div>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n    {{else}}\r\n        {{#unless required}}\r\n        <input class=\"form-radio\"\r\n               type=\"radio\"\r\n               name=\"attribute[{{id}}]\"\r\n               value=\"0\"\r\n               id=\"attribute_productlist_0_{{id}}\"\r\n               {{#if defaultValue '==' 0}}checked{{/if}}>\r\n        <label class=\"form-label\" for=\"attribute_productlist_0_{{id}}\">{{lang 'products.none'}}</label>\r\n        {{/unless}}\r\n        {{#each values}}\r\n            <input\r\n                class=\"form-radio\"\r\n                type=\"radio\"\r\n                name=\"attribute[{{../id}}]\"\r\n                value=\"{{id}}\"\r\n                id=\"attribute_productlist_{{../id}}_{{id}}\"\r\n                {{#if selected}}checked{{/if}}\r\n                {{#if ../required}}required{{/if}}>\r\n            <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_productlist_{{../id}}_{{id}}\">{{label}}</label>\r\n        {{/each}}\r\n    {{/if}}\r\n</div>\r\n","components/products/options/set-radio":"<div class=\"form-field\" data-product-attribute=\"set-radio\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    {{#each this.values}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"attribute_radio_{{../id}}_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}>\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_radio_{{../id}}_{{id}}\">{{this.label}}</label>\n    {{/each}}\n</div>\n","components/products/options/set-rectangle":"<div class=\"form-field\" data-product-attribute=\"set-rectangle\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{this.display_name}}:\n        <span data-option-value></span>\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n    {{#each this.values}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"attribute_rectangle__{{../id}}_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}>\n        <label class=\"form-option\" for=\"attribute_rectangle__{{../id}}_{{id}}\" data-product-attribute-value=\"{{id}}\">\n            <span class=\"form-option-variant\">{{this.label}}</span>\n        </label>\n    {{/each}}\n</div>\n","components/products/options/set-select":"<div class=\"form-field\" data-product-attribute=\"set-select\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_select_{{id}}\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <select class=\"form-select form-select--small\" name=\"attribute[{{this.id}}]\" id=\"attribute_select_{{id}}\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'products.choose_options'}}</option>\n        {{#each this.values}}\n            <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected data-default{{/if}}>{{label}}</option>\n        {{/each}}\n    </select>\n</div>\n","components/products/options/swatch":"<div class=\"form-field\" data-product-attribute=\"swatch\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{this.display_name}}:\n        <span data-option-value></span>\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    {{#each this.values}}\n        <input class=\"form-radio\" type=\"radio\" name=\"attribute[{{../id}}]\" value=\"{{id}}\" id=\"attribute_swatch_{{../id}}_{{id}}\" {{#if selected}}checked data-default{{/if}} {{#if ../required}}required{{/if}}>\n        <label class=\"form-option form-option-swatch\" for=\"attribute_swatch_{{../id}}_{{id}}\" data-product-attribute-value=\"{{id}}\">\n            {{#if image}}\n                <span class='form-option-variant form-option-variant--pattern' title=\"{{this.label}}\" style=\"background-image: url('{{getImage image \"swatch_option_size\"}}');\"></span>\n            {{/if}}\n            {{#if data.[2]}}\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[2]}}\"></span>\n            {{else}}\n                {{#if data.[1]}}\n                    <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                    <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\n                {{else}}\n                    {{#if data.[0]}}\n                        <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                    {{/if}}\n                {{/if}}\n            {{/if}}\n            {{#if pattern}}\n                <span class=\"form-option-expanded\">\n                    <span class=\"form-option-image\" style=\"background-image: url('{{getImage image 'core-swatch'}}');\"></span>\n                </span>\n            {{/if}}\n        </label>\n    {{/each}}\n</div>\n","components/products/options/textarea":"<div class=\"form-field\" data-product-attribute=\"textarea\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_textarea_{{id}}\">\n        {{display_name}}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <textarea class=\"form-input\" id=\"attribute_textarea_{{id}}\" name=\"attribute[{{id}}]\" {{#if required}}required{{/if}} {{#if max_length}}maxlength={{max_length}}{{/if}}>{{prefill}}</textarea>\n</div>\n"}
