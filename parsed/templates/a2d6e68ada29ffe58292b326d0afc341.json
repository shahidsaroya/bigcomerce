{"components/category/subcategories":"<section class=\"subcategories-carousel row\">\r\n    {{#each categories}}\r\n    \t{{#if @index '<=' 6 }}\r\n            <div class=\"tt-categories subcategories-{{id}}\">\r\n                <div class=\"content\">\r\n                    <div class=\"image\">\r\n                        <a class=\"subcat-img\" href=\"{{url}}\" >\r\n                        \t{{#if image}}\r\n\t                            <img class=\"lazyload\" data-sizes=\"auto\" alt=\"{{categories.alt}}\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage image 'data-sizes'}}\">\r\n                            {{else}}\r\n                               <img class=\"lazyload\" data-sizes=\"auto\" alt=\"{{categories.alt}}\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-vptmq0v2zd/product_images/uploaded_images/no-image.jpg\">\r\n                            {{/if}}\r\n                        </a>\r\n                    </div>\r\n                    <div class=\"caption\">\r\n                        <div class=\"cat-title\">\r\n                            <h4><a href=\"{{url}}\" data-hover=\"{{name}}\">{{name}}</a></h4>\r\n                            <p class=\"cat-product-count\">{{count}}{{lang 'categories.wdg-item'}}</p>\r\n                            <span class=\"material-icons\">keyboard_arrow_right</span>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        {{/if}}\r\n    {{/each}}\t\r\n</section>\r\n\r\n    \r\n"}
